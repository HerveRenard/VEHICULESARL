{% extends 'base.html.twig' %}

{% block title %}Fournisseur{% endblock %}

{% block body %}
    <h1>Fournisseur</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ fournisseur.id }}</td>
            </tr>
            <tr>
                <th>Civilite</th>
                <td>{{ fournisseur.civilite }}</td>
            </tr>
            <tr>
                <th>NomFour</th>
                <td>{{ fournisseur.nomFour }}</td>
            </tr>
            <tr>
                <th>Nationality</th>
                <td>{{ fournisseur.nationality }}</td>
            </tr>
            <tr>
                <th>DateDN</th>
                <td>{{ fournisseur.dateDN ? fournisseur.dateDN|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Tel</th>
                <td>{{ fournisseur.tel }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ fournisseur.email }}</td>
            </tr>
            <tr>
                <th>Ville</th>
                <td>{{ fournisseur.ville }}</td>
            </tr>
            <tr>
                <th>Addresse</th>
                <td>{{ fournisseur.addresse }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_fournisseur_index') }}">back to list</a>

    <a href="{{ path('app_fournisseur_edit', {'id': fournisseur.id}) }}">edit</a>

    {{ include('fournisseur/_delete_form.html.twig') }}
