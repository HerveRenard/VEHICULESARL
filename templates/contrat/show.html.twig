{% extends 'base.html.twig' %}

{% block title %}Contrat{% endblock %}

{% block body %}
    <h1>Contrat</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ contrat.id }}</td>
            </tr>
            <tr>
                <th>DateS</th>
                <td>{{ contrat.dateS ? contrat.dateS|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>DateD</th>
                <td>{{ contrat.dateD ? contrat.dateD|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>DateF</th>
                <td>{{ contrat.dateF ? contrat.dateF|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Libelle</th>
                <td>{{ contrat.libelle }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_contrat_index') }}">back to list</a>

    <a href="{{ path('app_contrat_edit', {'id': contrat.id}) }}">edit</a>

    {{ include('contrat/_delete_form.html.twig') }}
{% endblock %}
